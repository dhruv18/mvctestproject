@model IEnumerable<MVC_Testing.Models.employee>
@{
    ViewBag.Title = "Index";
}




<table class="table table-striped" style="width:50%">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Department</th>
    </tr>

    <tbody>
        @if (Model != null)
        {
            foreach(var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.name</td>
                    <td>@item.department</td>
                </tr>
            }
        }
    </tbody>
</table>




















